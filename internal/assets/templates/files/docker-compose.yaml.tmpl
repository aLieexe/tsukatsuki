services:
{{range .Service}}  {{.}}{{end}}
  app:
    image: {{ .ProjectName }}:latest
    build:
      context: ../../
      dockerfile: deploy/conf/Dockerfile
    healthcheck:
      test: ["CMD", "wget", "--spider", "-q", "http://localhost:5500/"]
      interval: 10s
      timeout: 3s
      retries: 3

volumes:
{{range .Volumes}}  {{.}}:
{{end}}